{
    "spec_dir": "spec",
  
    "spec_files": [
      "**/*[sS]pec.?(m)js",
      "**/*[sS]pec.?(m)ts",
      "!**/*nospec.js",
      "!**/*nospec.ts"
    ],
  
    "helpers": [
      "helpers/**/*.?(m)js",
      "helpers/**/*.?(m)ts"
    ],
    
    "env": {

      "failSpecWithNoExpectations": false,
      

      "stopSpecOnExpectationFailure": false,
  

      "stopOnSpecFailure": false,
  

      "random": false
    }
  }